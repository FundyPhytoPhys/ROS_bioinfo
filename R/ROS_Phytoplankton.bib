@article{bondHeterotrophicBacteriaExhibit2020,
  title = {Heterotrophic {{Bacteria Exhibit}} a {{Wide Range}} of {{Rates}} of {{Extracellular Production}} and {{Decay}} of {{Hydrogen Peroxide}}},
