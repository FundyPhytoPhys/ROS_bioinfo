@article{olasehindePhotochemicalProductionConsumption2010,
  title = {Photochemical {{Production}} and {{Consumption Mechanisms}} of {{Nitric Oxide}} in {{Seawater}}},
  author = {Olasehinde, Emmanuel F. and Takeda, Kazuhiko and Sakugawa, Hiroshi},
  year = {2010},
  month = nov,
  journal = {Environ. Sci. Technol.},
  volume = {44},
  number = {22},
  pages = {8403--8408},
  publisher = {{American Chemical Society}},
  issn = {0013-936X},
  doi = {10.1021/es101426x},
  abstract = {Nitric oxide (NO\textbullet ) is an active odd-nitrogen species that plays a critical role in determining the levels of ozone (O3) and other nitrogen species in the troposphere. Here, we provide experimental evidence for photochemical formation of NO\textbullet{} in seawater. Photoproduction rates and overall scavenging rate constants were measured by irradiation of surface seawater samples collected from the Seto Inland Sea, Japan. Photoproduction rates of NO\textbullet{} ranged from 8.7 \texttimes{} 10-12 M s-1 to 38.8 \texttimes{} 10-12 M s-1 and scavenging rate constants were 0.05-0.33 s-1. The steady state concentrations of NO\textbullet{} in seawater, which were calculated from the photoproduction rates and scavenging rate constants were in the range 2.4-32 \texttimes{} 10-11 M. Estimation from the scavenging rate constant showed that the NO\textbullet{} lifetime in seawater was a few seconds. Our results indicate that nitrite photolysis plays a crucial role in the formation of NO\textbullet, even though we cannot exclude minor contributions from other sources. Analysis of filtered and unfiltered seawater samples showed no significant difference in NO\textbullet{} photoformation rates, which suggests a negligible contribution of NO\textbullet{} produced by photobiological processes. Using an estimated value of the Henry's law constant (kH {$\approx$} 0.0019 M atm-1), a supersaturation of surface seawater of 2 to 3 orders of magnitude was estimated. On the basis of the average values of the surface seawater concentration and the atmospheric NO\textbullet{} concentration, a sea-to-air NO\textbullet{} flux was estimated.},
  file = {/Users/naamanomar/Zotero/storage/5CLS39SE/Olasehinde et al. - 2010 - Photochemical Production and Consumption Mechanism.pdf;/Users/naamanomar/Zotero/storage/V5I8KRBB/es101426x.html}
}

