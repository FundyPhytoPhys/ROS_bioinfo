@article{nawrockiPlastidTerminalOxidase2015,
  title = {The {{Plastid Terminal Oxidase}}: {{Its Elusive Function Points}} to {{Multiple Contributions}} to {{Plastid Physiology}}},
  shorttitle = {The {{Plastid Terminal Oxidase}}},
  author = {Nawrocki, Wojciech J. and Tourasse, Nicolas J. and Taly, Antoine and Rappaport, Fabrice and Wollman, Francis-Andr{\'e}},
  year = {2015},
  month = apr,
  journal = {Annual Review of Plant Biology},
  volume = {66},
  number = {1},
  pages = {49--74},
  issn = {1543-5008, 1545-2123},
  doi = {10.1146/annurev-arplant-043014-114744},
  abstract = {Plastids have retained from their cyanobacterial ancestor a fragment of the respiratory electron chain comprising an NADPH dehydrogenase and a diiron oxidase, which sustain the so-called chlororespiration pathway. Despite its very low turnover rates compared with photosynthetic electron flow, knocking out the plastid terminal oxidase (PTOX) in plants or microalgae leads to severe phenotypes that encompass developmental and growth defects together with increased photosensitivity. On the basis of a phylogenetic and structural analysis of the enzyme, we discuss its physiological contribution to chloroplast metabolism, with an emphasis on its critical function in setting the redox poise of the chloroplast stroma in darkness. The emerging picture of PTOX is that of an enzyme at the crossroads of a variety of metabolic processes, such as, among others, the regulation of cyclic electron transfer and carotenoid biosynthesis, which have in common their dependence on the redox state of the plastoquinone pool, set largely by the activity of PTOX in darkness.},
  langid = {english},
  file = {/Users/naamanomar/Zotero/storage/I99RAT47/Nawrocki et al. - 2015 - The Plastid Terminal Oxidase Its Elusive Function.pdf}
