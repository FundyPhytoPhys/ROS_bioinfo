@article{richardsHydroxylRadicalPlants2015,
  title = {The Hydroxyl Radical in Plants: From Seed to Seed},
  shorttitle = {The Hydroxyl Radical in Plants},
  author = {Richards, Si{\^a}n L. and Wilkins, Katie A. and Swarbreck, St{\'e}phanie M. and Anderson, Alexander A. and Habib, Noman and Smith, Alison G. and McAinsh, Martin and Davies, Julia M.},
  year = {2015},
  month = jan,
  journal = {Journal of Experimental Botany},
  volume = {66},
  number = {1},
  pages = {37--46},
  issn = {0022-0957},
  doi = {10.1093/jxb/eru398},
  abstract = {The hydroxyl radical (OH\textbullet ) is the most potent yet short-lived of the reactive oxygen species (ROS) radicals. Just as hydrogen peroxide was once considered to be simply a deleterious by-product of oxidative metabolism but is now acknowledged to have signalling roles in plant cells, so evidence is mounting for the hydroxyl radical as being more than merely an agent of destruction. Its oxidative power is harnessed to facilitate germination, growth, stomatal closure, reproduction, the immune response, and adaptation to stress. It features in plant cell death and is a key tool in microbial degradation of plant matter for recycling. Production of the hydroxyl radical in the wall, at the plasma membrane, and intracellularly is facilitated by a range of peroxidases, superoxide dismutases, NADPH oxidases, and transition metal catalysts. The spatio-temporal activity of these must be tightly regulated to target substrates precisely to the site of radical production, both to prevent damage and to accommodate the short half life and diffusive capacity of the hydroxyl radical. Whilst research has focussed mainly on the hydroxyl radical's mode of action in wall loosening, studies now extend to elucidating which proteins are targets in signalling systems. Despite the difficulties in detecting and manipulating this ROS, there is sufficient evidence now to acknowledge the hydroxyl radical as a potent regulator in plant cell biology.},
  file = {/Users/naamanomar/Zotero/storage/8CWB5JTQ/Richards et al. - 2015 - The hydroxyl radical in plants from seed to seed.pdf;/Users/naamanomar/Zotero/storage/UVWNKM9U/2893394.html}
}

