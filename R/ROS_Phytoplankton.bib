  title = {{{RNA-Seq Analysis Reveals Genes Related}} to {{Photoreception}}, {{Nutrient Uptake}}, and {{Toxicity}} in a {{Noxious Red-Tide Raphidophyte}} {{{\emph{Chattonella}}}}{\emph{ Antiqua}}},
  author = {Shikata, Tomoyuki and Takahashi, Fumio and Nishide, Hiroyo and Shigenobu, Shuji and Kamei, Yasuhiro and Sakamoto, Setsuko and Yuasa, Kouki and Nishiyama, Yoshitaka and Yamasaki, Yasuhiro and Uchiyama, Ikuo},
