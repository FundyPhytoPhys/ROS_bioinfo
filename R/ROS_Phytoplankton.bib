}

@article{formanWhatConcentrationHydrogen2016,
  title = {What Is the Concentration of Hydrogen Peroxide in Blood and Plasma?},
  author = {Forman, Henry Jay and Bernardo, Angelito and Davies, Kelvin J. A.},
