@article{stromMicrozooplanktonGrazingCoastal2007,
  title = {Microzooplankton Grazing in the Coastal {{Gulf}} of {{Alaska}}: {{Variations}} in Top-down Control of Phytoplankton},
  shorttitle = {Microzooplankton Grazing in the Coastal {{Gulf}} of {{Alaska}}},
  author = {Strom, Suzanne L. and Macri, Erin L. and Olson, M. Brady},
  year = {2007},
  journal = {Limnol. Oceanogr.},
  volume = {52},
  number = {4},
  pages = {1480--1494},
  issn = {1939-5590},
  doi = {10.4319/lo.2007.52.4.1480},
  abstract = {Microzooplankton grazing rates on three phytoplankton size fractions ({$<$}5, 5-20, and {$>$}20 \textmu m) were measured during spring and summer 2001 in the northern coastal Gulf of Alaska (CGOA). To a first approximation, microzooplankton consumed all production by phytoplankton {$<$}20 \textmu m in size and nearly half the production by phytoplankton {$>$}20 \textmu m, mainly diatoms. Microzooplankton (ciliate plus heterotrophic dinoflagellate) biomass ranged from 9.6 \textmu g C L-1 to 82.2 \textmu g C L-1. The highest levels were associated with diatom blooms and equaled those previously reported for highly productive coastal upwelling regions. Regulation of microzooplankton grazing differed according to size class. Grazing on phytoplankton {$<$}5 \textmu m in size averaged 0.48 d-1 and was closely correlated with phytoplankton growth rates in the same size class. In contrast, grazing on phytoplankton {$>$}20 \textmu m averaged 0.17 d-1 and was unrelated to phytoplankton growth rate in this size class. Variations in grazing pressure on these largest phytoplankton arose mainly through variations in the biomass of the larger ({$>$}40 \textmu m) ciliates and dinoflagellates. This biomass, in turn, became more closely correlated with {$>$}20 \textmu m chlorophyll as the season progressed, indicating removal of top-down control on these ciliates and dinoflagellates as Neocalanus spp. copepods left the upper water column. Because microzooplankton directly consume much of the phytoplankton production in the CGOA, processes that regulate this trophic linkage have major implications for food web structure and secondary production in this coastal ecosystem.},
  langid = {english},
  annotation = {\_eprint: https://aslopubs.onlinelibrary.wiley.com/doi/pdf/10.4319/lo.2007.52.4.1480},
  file = {/Users/naamanomar/Zotero/storage/NNG2C56V/Strom et al. - 2007 - Microzooplankton grazing in the coastal Gulf of Al.pdf;/Users/naamanomar/Zotero/storage/FI5XN4AF/lo.2007.52.4.html}
}

