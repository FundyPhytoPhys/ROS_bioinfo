@article{xuEvidenceCoexistenceC32012,
  title = {Evidence of {{Coexistence}} of {{C3}} and {{C4 Photosynthetic Pathways}} in a {{Green-Tide-Forming Alga}}, {{Ulva}} Prolifera},
