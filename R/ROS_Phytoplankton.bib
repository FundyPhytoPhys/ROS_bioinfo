@article{wangImagingROSSignaling2013,
  title = {Imaging {{ROS}} Signaling in Cells and Animals},
  author = {Wang, Xianhua and Fang, Huaqiang and Huang, Zhanglong and Shang, Wei and Hou, Tingting and Cheng, Aiwu and Cheng, Heping},
  year = {2013},
  journal = {J Mol Med (Berl)},
  volume = {91},
  number = {8},
  pages = {917--927},
  issn = {0946-2716},
  doi = {10.1007/s00109-013-1067-4},
  abstract = {Reactive oxygen species (ROS) act as essential cellular messengers, redox regulators, and, when in excess, oxidative stressors that are widely implicated in pathologies of cancer and cardiovascular and neurodegenerative diseases. Understanding such complexity of the ROS signaling is critically hinged on the ability to visualize and quantify local, compartmental, and global ROS dynamics at high selectivity, sensitivity, and spatiotemporal resolution. The past decade has witnessed significant progress in ROS imaging at levels of intact cells, whole organs or tissues, and even live organisms. In particular, major advances include the development of novel synthetic or genetically encoded fluorescent protein-based ROS indicators, the use of protein indicator-expressing animal models, and the advent of in vivo imaging technology. Innovative ROS imaging has led to important discoveries in ROS signaling\textemdash for example, mitochondrial superoxide flashes as elemental ROS signaling events and hydrogen peroxide transients for wound healing. This review aims at providing an update of the current status in ROS imaging, while identifying areas of insufficient knowledge and highlighting emerging research directions.},
  pmcid = {PMC3730091},
  pmid = {23873151},
  file = {/Users/naamanomar/Zotero/storage/JTP644M4/Wang et al. - 2013 - Imaging ROS signaling in cells and animals.pdf}
}

@article{wangMicroalgalAssemblagesPoikilohaline2014,
  title = {Microalgal Assemblages in a Poikilohaline Pond},
  author = {Wang, Shuyi and Lambert, William and Giang, Sophia and Goericke, Ralf and Palenik, Brian},
  year = {2014},
  journal = {J. Phycol.},
  volume = {50},
  number = {2},
  pages = {303--309},
  issn = {1529-8817},
  doi = {10.1111/jpy.12158},
  abstract = {Microalgal strains for algal biofuels production in outdoor ponds will need to have high net growth rates under diverse environmental conditions. A small, variable salinity pond in the San Elijo Lagoon estuary in southern California was chosen to serve as a model pond due to its routinely high chlorophyll content. Profiles of microalgal assemblages from water samples collected from April 2011 to January 2012 were obtained by constructing 18S rDNA environmental clone libraries. Pond assemblages were found to be dominated by green algae Picochlorum sp. and Picocystis sp. throughout the year. Pigment analysis suggested that the two species contributed most of the chlorophyll a of the pond, which ranged from 21.9 to 664.3 {$\mu$}g {$\cdot$} L-1 with the Picocystis contribution increasing at higher salinities. However, changes of temperature, salinity or irradiance may have enabled a bloom of the diatom Chaetoceros sp. in June 2011. Isolates of these microalgae were obtained and their growth rates characterized as a function of temperature and salinity. Chaetoceros sp. had the highest growth rate over the temperature test range while it showed the most sensitivity to high salinity. All three strains showed the presence of lipid bodies during nitrogen starvation, suggesting they have potential as future biofuels strains.},
  langid = {english},
  keywords = {18S rDNA libraries,algal biofuels,Chaetoceros,Picochlorum,Picocystis,poikilohaline pond},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jpy.12158},
