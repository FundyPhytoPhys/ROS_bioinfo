@article{iovinellaCrypticDispersalCyanidiophytina2018,
  title = {Cryptic Dispersal of {{Cyanidiophytina}} ({{Rhodophyta}}) in Non-Acidic Environments from {{Turkey}}},
  author = {Iovinella, Manuela and Eren, Ayla and Pinto, Gabriele and Pollio, Antonino and Davis, Seth J. and Cennamo, Paola and Ciniglia, Claudia},
  year = {2018},
  month = sep,
  journal = {Extremophiles},
  volume = {22},
  number = {5},
