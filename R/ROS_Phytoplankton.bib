@article{zhuNitricOxideDismutase2019,
  title = {Nitric {{Oxide Dismutase}} (Nod) {{Genes}} as a {{Functional Marker}} for the {{Diversity}} and {{Phylogeny}} of {{Methane-Driven Oxygenic Denitrifiers}}},
  author = {Zhu, Baoli and Wang, Jiaqi and Bradford, Lauren M. and Ettwig, Katharina and Hu, Baolan and Lueders, Tillmann},
  year = {2019},
  journal = {Front. Microbiol.},
  volume = {10},
  publisher = {{Frontiers}},
  issn = {1664-302X},
  doi = {10.3389/fmicb.2019.01577},
  abstract = {Oxygenic denitrification represents a new route in reductive nitrogen turnover which differs from canonical denitrification in how nitric oxide (NO) is transformed into dinitrogen gas. Instead of NO reduction via N2O to N2, NO is proposed to be directly disproportionated into N2 and O2 in oxygenic denitrification, catalyzed by the putative NO dismutase (Nod). Although a high diversity of nod genes has been recovered from various environments, still little is known about the niche partitioning and ecophysiology of oxygenic denitrifiers. One constraint is that nod as a functional marker for oxygenic denitrifiers is not well established. To address this issue, we compared the diversity and phylogeny of nod, 16S rRNA and pmoA gene sequences of four NC10 enrichments that are capable of methane-driven oxygenic denitrification and one environmental sample. The phylogenies of nod, 16S rRNA and pmoA genes of these cultures were generally congruent. And in each sample, the NC10 bacteria diversity inferred from different genes was also similar. A new set of NC10-specific nod primers was developed and used in qPCR. The abundance of NC10 bacteria inferred from nod genes was constantly lower than via 16S rRNA genes, but the difference was within one order of magnitude. These results suggest that nod is a suitable molecular marker for studying the diversity and phylogeny of methane-driven oxygenic denitrifiers, the further investigation of which may be of value to develop enhanced strategies for sustainable nitrogen or methane removal.},
  langid = {english},
  keywords = {Functional marker,NC10,Nitric oxide dismutation,NOD,oxygenic denitrification,oxygenic methanotrophs,pmoA}
}

@article{zielonkaPeroxynitriteMajorSpecies2010,
  title = {Peroxynitrite {{Is}} the {{Major Species Formed}} from {{Different Flux Ratios}} of {{Co-generated Nitric Oxide}} and {{Superoxide}}: {{DIRECT REACTION WITH BORONATE-BASED FLUORESCENT PROBE}}},
  shorttitle = {Peroxynitrite {{Is}} the {{Major Species Formed}} from {{Different Flux Ratios}} of {{Co-generated Nitric Oxide}} and {{Superoxide}}},
  author = {Zielonka, Jacek and Sikora, Adam and Joseph, Joy and Kalyanaraman, Balaraman},
  year = {2010},
  month = may,
  journal = {J. Biol. Chem.},
  volume = {285},
  number = {19},
  pages = {14210--14216},
  publisher = {{Elsevier}},
  issn = {0021-9258, 1083-351X},
  doi = {10.1074/jbc.M110.110080},
  abstract = {{$<$}p{$>$}There is much interest in the nitration and oxidation reaction mechanisms initiated by superoxide radical anion (O\textsubscript{2}\textsuperscript{\.\=}) and nitric oxide (\textsuperscript{\textbullet}NO). It is well known that O\textsubscript{2}\textsuperscript{\.\=} and \textsuperscript{\textbullet}NO rapidly react to form a potent oxidant, peroxynitrite anion (ONOO\textsuperscript{-}). However, indirect measurements with the existing probes (\emph{e.g.} dihydrorhodamine) previously revealed a bell-shaped response to co-generated \textsuperscript{\textbullet}NO and O\textsubscript{2}\textsuperscript{\.\=} fluxes, with the maximal yield of the oxidation or nitration product occurring at a 1:1 ratio. These results raised doubts on the formation of ONOO\textsuperscript{-} \emph{per se} at various fluxes of \textsuperscript{\textbullet}NO and O\textsubscript{2}\textsuperscript{\.\=}. Using a novel fluorogenic probe, coumarin-7-boronic acid, that reacts stoichiometrically and rapidly with ONOO\textsuperscript{-} (\emph{k} = 1.1 \texttimes{} 10\textsuperscript{6} m\textsuperscript{-1}s\textsuperscript{-1}), we report that ONOO\textsuperscript{-} formation increased linearly and began to plateau after reaching a 1:1 ratio of co-generated \textsuperscript{\textbullet}NO and O\textsubscript{2}\textsuperscript{\.\=} fluxes. We conclude that ONOO\textsuperscript{-} is formed as the primary intermediate during the reaction between \textsuperscript{\textbullet}NO and O\textsubscript{2}\textsuperscript{\.\=} co-generated at different fluxes.{$<$}/p{$>$}},
  langid = {english},
  pmid = {20194496},
  file = {/Users/naamanomar/Zotero/storage/XUQMQ4PC/Zielonka et al. - 2010 - Peroxynitrite Is the Major Species Formed from Dif.pdf;/Users/naamanomar/Zotero/storage/HR6FJNZT/fulltext.html}
}

