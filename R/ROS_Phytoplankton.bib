@article{wangQuantifyingCellularOxidative1999,
  title = {Quantifying Cellular Oxidative Stress by Dichlorofluorescein Assay Using Microplate Reader},
  author = {Wang, H. and Joseph, J. A.},
  year = {1999},
  month = sep,
  journal = {Free Radical Biology \& Medicine},
  volume = {27},
  number = {5-6},
  pages = {612--616},
  issn = {0891-5849},
  doi = {10.1016/s0891-5849(99)00107-0},
  abstract = {Oxidative stress (OS) has been implicated in various degenerative diseases in aging. In an attempt to quantify OS in a cell model, we examined OS induced by incubating for 30 min with various free radical generators in PC12 cells by using the dichlorofluorescein (DCF) assay, modified for use by a fluorescent microplate reader. The nonfluorescent fluorescin derivatives (dichlorofluorescin, DCFH), after being oxidized by various oxidants, will become DCF and emit fluorescence. By quantifying the fluorescence, we were able to quantify the OS. Our results indicated that the fluorescence varied linearly with increasing concentrations (between 0.1 and 1 mM) of H2O2 and 2,2'-azobios(2-amidinopropane) dihydrochloride (AAPH; a peroxyl radical generator). By contrast, the fluorescence varied as a nonlinear response to increasing concentrations of 3-morpholinosydnonimine hydrochloride (SIN-1; a peroxynitrite generator), sodium nitroprusside (SNP; a nitric oxide generator), and dopamine. Dopamine had a biphasic effect; it decreased the DCF fluorescence, thus acting as an antioxidant, at concentrations {$<$}500 microM in cells, but acted as a pro-oxidant by increasing the fluorescence at 1 mM. While SNP was not a strong pro-oxidant, SIN-1 was the most potent pro-oxidant among those tested, inducing a 70 times increase of fluorescence at a concentration of 100 microM compared with control. Collectively, due to its indiscriminate nature to various free radicals, DCF can be very useful in quantifying overall OS in cells, especially when used in conjunction with a fluorescent microplate reader. This method is reliable and efficient for evaluating the potency of pro-oxidants and can be used to evaluate the efficacy of antioxidants against OS in cells.},
  langid = {english},
  pmid = {10490282},
  keywords = {Amidines,Animals,Dopamine,Fluoresceins,Fluorescent Dyes,Free Radicals,Hydrogen Peroxide,Molsidomine,Nitric Oxide Donors,Nitroprusside,Oxidants,Oxidative Stress,PC12 Cells,Rats}
}

@article{wangRoleNitrogenGrowth2011,
  title = {The Role of Nitrogen on the Growth and Colony Development of {{{\emph{Phaeocystis}}}}{\emph{ Globosa}} ({{Prymnesiophyceae}})},
  author = {Wang, Xiaodong and Wang, Yan and Jr, Walker O. Smith},
