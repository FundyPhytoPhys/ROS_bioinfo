  file = {/Users/naamanomar/Zotero/storage/YCKHGEPQ/Zeileis et al. - 2008 - Regression Models for Count Data in R.pdf}
}

@article{zhangExtensiveDarkBiological2016,
  title = {Extensive {{Dark Biological Production}} of {{Reactive Oxygen Species}} in {{Brackish}} and {{Freshwater Ponds}}},
  author = {Zhang, Tong and Hansel, Colleen M. and Voelker, Bettina M. and Lamborg, Carl H.},
  year = {2016},
  month = mar,
  journal = {Environ. Sci. Technol.},
  volume = {50},
  number = {6},
  pages = {2983--2993},
  publisher = {{American Chemical Society}},
  issn = {0013-936X},
  doi = {10.1021/acs.est.5b03906},
  abstract = {Within natural waters, photodependent processes are generally considered the predominant source of reactive oxygen species (ROS), a suite of biogeochemically important molecules. However, recent discoveries of dark particle-associated ROS production in aquatic environments and extracellular ROS production by various microorganisms point to biological activity as a significant source of ROS in the absence of light. Thus, the objective of this study was to explore the occurrence of dark biological production of the ROS superoxide (O2\textendash ) and hydrogen peroxide (H2O2) in brackish and freshwater ponds. Here we show that the ROS superoxide and hydrogen peroxide were present in dark waters at comparable concentrations as in sunlit waters. This suggests that, at least for the short-lived superoxide species, light-independent processes were an important control on ROS levels in these natural waters. Indeed, we demonstrated that dark biological production of ROS extensively occurred in brackish and freshwater environments, with greater dark ROS production rates generally observed in the aphotic relative to the photic zone. Filtering and formaldehyde inhibition confirmed the biological nature of a majority of this dark ROS production, which likely involved phytoplankton, particle-associated heterotrophic bacteria, and NADH-oxidizing enzymes. We conclude that biological ROS production is widespread, including regions devoid of light, thereby expanding the relevance of these reactive molecules to all regions of our oxygenated global habit.}
}

@article{zhangMorphologyUltrastructurePhylogeny2018,
