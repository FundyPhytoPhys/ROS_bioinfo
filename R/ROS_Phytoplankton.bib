@article{latifiOxidativeStressCyanobacteria2009a,
  ids = {latifiOxidativeStressCyanobacteria2009},
  title = {Oxidative Stress in Cyanobacteria},
  author = {Latifi, Amel and Ruiz, Marion and Zhang, Cheng-Cai},
