@article{andersenModellingEmergentTrophic2015,
  title = {Modelling Emergent Trophic Strategies in Plankton},
  author = {Andersen, Ken H. and Aksnes, Dag L. and Berge, Terje and Fiksen, {\O}yvind and Visser, Andr{\'e}},
