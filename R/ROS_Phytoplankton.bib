@article{shimizuReactionSuperoxideRadical1984,
  title = {The Reaction of Superoxide Radical with Catalase. {{Mechanism}} of the Inhibition of Catalase by Superoxide Radical},
  author = {Shimizu, N. and Kobayashi, K. and Hayashi, K.},
  year = {1984},
