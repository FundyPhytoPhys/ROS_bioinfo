@article{perryPhotoadaptionMarinePhytoplankton1981,
  title = {Photoadaption in Marine Phytoplankton: {{Response}} of the Photosynthetic Unit},
  shorttitle = {Photoadaption in Marine Phytoplankton},
  author = {Perry, M. J. and Talbot, M. C. and Alberte, R. S.},
  year = {1981},
  month = may,
  journal = {Mar. Biol.},
  volume = {62},
