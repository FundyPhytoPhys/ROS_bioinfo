@article{zafiriouChemistrySuperoxideRadical1998,
  title = {Chemistry of the {{Superoxide Radical}} ({{O2-}}) in {{Seawater}}:\, {{Reactions}} with {{Inorganic Copper Complexes}}},
  shorttitle = {Chemistry of the {{Superoxide Radical}} ({{O2-}}) in {{Seawater}}},
  author = {Zafiriou, Oliver C. and Voelker, Bettina M. and Sedlak, David L.},
  year = {1998},
  month = jul,
  journal = {J. Phys. Chem. A},
  volume = {102},
  number = {28},
  pages = {5693--5700},
  publisher = {{American Chemical Society}},
  issn = {1089-5639},
  doi = {10.1021/jp980709g},
  abstract = {Copper-catalyzed dismutation has been proposed as an important sink of photoproduced superoxide radical (O2-) in seawater. We have determined the effective rate constants of the reactions of superoxide with Cu(I) and Cu(II) in seawater, using pulse radiolysis and kinetic spectrophotometry. Due to the effects of ionic strength and complexation, the observed rate constants, 1.98({$\pm$}0.05) \texttimes{} 109 M-1 s-1 and 0.663({$\pm$}0.071) \texttimes{} 109 M-1 s-1, for Cu(I) and Cu(lI), respectively, are 1 order of magnitude lower than those reported for Cu+ and Cu2+ in low ionic strength media. Measurements of the rate of O2- decay in the presence of catalytic concentrations of dissolved Cu agreed well with predictions based on the independently measured rate constants. Observed steady-state concentrations of Cu(I) in solutions exposed to a constant flux of O2- were also consistent with the measured rate constants. From these results and available data regarding Cu speciation in coastal and open oceans, we calculate that O2- reactions with Cu will be a more important sink of O2- than bimolecular dismutation, even if all organically complexed Cu species are unreactive.},
  file = {/Users/naamanomar/Zotero/storage/6UBXR4NQ/Zafiriou et al. - 1998 - Chemistry of the Superoxide Radical (O2-) in Seawa.pdf;/Users/naamanomar/Zotero/storage/DQKFWJ6A/jp980709g.html}
}

@article{zafiriouNitricOxideNitrite1981,
  title = {Nitric Oxide from Nitrite Photolysis in the Central Equatorial {{Pacific}}},
  author = {Zafiriou, O. C. and McFarland, M.},
  year = {1981},
  journal = {Journal of Geophysical Research: Oceans},
  volume = {86},
  number = {C4},
  pages = {3173--3182},
  issn = {2156-2202},
  doi = {10.1029/JC086iC04p03173},
  abstract = {Sunlight photolyzes nitrite in seawater: NO2- + HOH + hv = NO + OH + OH-. We studied nitrite loss and nitric oxide production attributed to this reaction in surface waters of the equatorial Pacific near 170\textdegree W. Net photochemical loss rates of 2\textendash 15\% per day were derived from two different types of laboratory incubation experiments. The net nitrite loss rate in the surface water of this region is calculated to average 4 \texttimes{} 10-13 mol l-l s-l during the day, or {$\sim$}6 \texttimes{} 10-2 mol m-2 y-1. Nitric oxide was detected in situ with a floating gas-seawater equilibrator. NO was always detectable in nitrite-containing seawater during the day but was undetectable at night or in nitrite-free water. Near sunrises and sunsets the estimated NO vapor pressure, pNO(sea) covaried with the ambient UV insolation in air according to log pNO(sea) = a log UVair + b. Best-fit values to the in situ data indicate a {$\approx$} 1 with r2 {$\geq$} 0.9; simple kinetic models rationalize a values of O, {$\frac{1}{2}$}, or 1. During the day, pNO(sea) averaged {$\sim$}3.1 \texttimes{} 10-8 atm, corresponding to {$\sim$}4.6 \texttimes{} 10-11 M [NO]aq. The ambient atmospheric pNO was {$\sim$}104 -fold lower, implying a substantial seawater supersaturation and a sea \textrightarrow{} air flux. From the stagnant-boundary layer model and our measurements, we estimate {$\sim$}2 \texttimes{} 10-16 mol 1-1 s-1 ({$\sim$}1.3 \texttimes{} 108 molecule cm-2 s-1) of NO efflux in daylight, an insignificant NO loss from the sea. The photochemical NO source and the estimated dark reaction sink are, within the accuracy of the data, in balance. These results provide evidence for the presence of NO, a free radical, in surface seawater. They substantiate that photochemical reactions produce measurable concentrations of reactive intermediates in surface seawater and that these enter into rapid secondary reactions. These processes may reach sufficient intensity to provide significant effects, such as sea \textrightarrow{} air fluxes.},
  langid = {english},
  annotation = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/JC086iC04p03173},
  file = {/Users/naamanomar/Zotero/storage/6VGASMZT/Zafiriou and McFarland - 1981 - Nitric oxide from nitrite photolysis in the centra.pdf;/Users/naamanomar/Zotero/storage/I3IRRMQR/JC086iC04p03173.html}
}

