@article{weiPicophytoplanktonSizeBiomass2018,
  title = {Picophytoplankton Size and Biomass around Equatorial Eastern {{Indian Ocean}}},
  author = {Wei, Yuqiu and Sun, Jun and Zhang, Xiaodong and Wang, Jing and Huang, Ke},
