  author = {Yu, Jingjie and Liberton, Michelle and Cliften, Paul F. and Head, Richard D. and Jacobs, Jon M. and Smith, Richard D. and Koppenaal, David W. and Brand, Jerry J. and Pakrasi, Himadri B.},
