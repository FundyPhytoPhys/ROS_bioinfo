@article{lipinskiHydroxylRadicalIts2011,
  title = {Hydroxyl {{Radical}} and {{Its Scavengers}} in {{Health}} and {{Disease}}},
  author = {Lipinski, Boguslaw},
  year = {2011},
  journal = {Oxid Med Cell Longev},
  volume = {2011},
