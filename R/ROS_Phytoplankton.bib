@article{martinezClimateDrivenBasinScaleDecadal2009,
  title = {Climate-{{Driven Basin-Scale Decadal Oscillations}} of {{Oceanic Phytoplankton}}},
  author = {Martinez, Elodie and Antoine, David and D'Ortenzio, Fabrizio and Gentili, Bernard},
