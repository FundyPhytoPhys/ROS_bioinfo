  month = mar,
  journal = {Nat Commun},
  volume = {9},
  number = {1},
  pages = {953},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/s41467-018-03376-9},
  abstract = {Diatoms are one of the major primary producers in the ocean, responsible annually for \textasciitilde 20\% of photosynthetically fixed CO2 on Earth. In oceanic models, they are typically represented as large ({$>$}20\,\textmu m) microphytoplankton. However, many diatoms belong to the nanophytoplankton (2\textendash 20\,\textmu m) and a few species even overlap with the picoplanktonic size-class ({$<$}2\,\textmu m). Due to their minute size and difficulty of detection they are poorly characterized. Here we describe a massive spring bloom of the smallest known diatom (Minidiscus) in the northwestern Mediterranean Sea. Analysis of Tara Oceans data, together with literature review, reveal a general oversight of the significance of these small diatoms at the global scale. We further evidence that they can reach the seafloor at high sinking rates, implying the need to revise our classical binary vision of pico- and nanoplanktonic cells fueling the microbial loop, while only microphytoplankton sustain secondary trophic levels and carbon export.},
  copyright = {2018 The Author(s)},
  langid = {english},
  keywords = {Carbon cycle,DNA sequencing,Marine biology},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Carbon cycle;DNA sequencing;Marine biology Subject\_term\_id: carbon-cycle;dna-sequencing;marine-biology},
  file = {/Users/naamanomar/Zotero/storage/JSMV47K2/Leblanc et al. - 2018 - Nanoplanktonic diatoms are globally overlooked but.pdf;/Users/naamanomar/Zotero/storage/U5ZCALIQ/s41467-018-03376-9.html}
}

@article{ledfordAcclimationSingletOxygen2007,
