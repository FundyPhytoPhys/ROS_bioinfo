  title = {Effects of Sunlight and Hydroxyl Radical on Dissolved Organic Matter: {{Bacterial}} Growth Efficiency and Production of Carboxylic Acids and Other Substrates},
  shorttitle = {Effects of Sunlight and Hydroxyl Radical on Dissolved Organic Matter},
  author = {Pullin, Michael J. and Bertilsson, Stefan and Goldstone, Jared V. and Voelker, Bettina M.},
