@article{krienitzFoodAlgaeLesser2016,
  title = {Food Algae for {{Lesser Flamingos}}: A Stocktaking},
  shorttitle = {Food Algae for {{Lesser Flamingos}}},
  author = {Krienitz, Lothar and Krienitz, Doris and Dadheech, Pawan K. and H{\"u}bener, Thomas and Kotut, Kiplagat and Luo, Wei and Teubner, Katrin and Versfeld, Wilferd D.},
