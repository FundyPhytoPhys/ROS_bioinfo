@article{munoz-marinMixotrophyMarinePicocyanobacteria2020,
  title = {Mixotrophy in Marine Picocyanobacteria: Use of Organic Compounds by {{{\emph{Prochlorococcus}}}} and {{{\emph{Synechococcus}}}}},
  shorttitle = {Mixotrophy in Marine Picocyanobacteria},
  author = {{Mu{\~n}oz-Mar{\'i}n}, M. C. and {G{\'o}mez-Baena}, G. and {L{\'o}pez-Lozano}, A. and {Moreno-Cabezuelo}, J. A. and D{\'i}ez, J. and {Garc{\'i}a-Fern{\'a}ndez}, J. M.},
