@article{murphyHowMitochondriaProduce2009,
  title = {How Mitochondria Produce Reactive Oxygen Species},
  author = {Murphy, Michael~P.},
  year = {2009},
  month = jan,
  journal = {Biochem J},
  volume = {417},
  number = {Pt 1},
  pages = {1--13},
  issn = {0264-6021},
  doi = {10.1042/BJ20081386},
  abstract = {The production of ROS (reactive oxygen species) by mammalian mitochondria is important because it underlies oxidative damage in many pathologies and contributes to retrograde redox signalling from the organelle to the cytosol and nucleus. Superoxide (O2\textbullet -) is the proximal mitochondrial ROS, and in the present review I outline the principles that govern O2\textbullet - production within the matrix of mammalian mitochondria. The flux of O2\textbullet - is related to the concentration of potential electron donors, the local concentration of O2 and the second-order rate constants for the reactions between them. Two modes of operation by isolated mitochondria result in significant O2\textbullet - production, predominantly from complex I: (i) when the mitochondria are not making ATP and consequently have a high {$\Delta$}p (protonmotive force) and a reduced CoQ (coenzyme Q) pool; and (ii) when there is a high NADH/NAD+ ratio in the mitochondrial matrix. For mitochondria that are actively making ATP, and consequently have a lower {$\Delta$}p and NADH/NAD+ ratio, the extent of O2\textbullet - production is far lower. The generation of O2\textbullet - within the mitochondrial matrix depends critically on {$\Delta$}p, the NADH/NAD+ and CoQH2/CoQ ratios and the local O2 concentration, which are all highly variable and difficult to measure in vivo. Consequently, it is not possible to estimate O2\textbullet - generation by mitochondria in vivo from O2\textbullet --production rates by isolated mitochondria, and such extrapolations in the literature are misleading. Even so, the description outlined here facilitates the understanding of factors that favour mitochondrial ROS production. There is a clear need to develop better methods to measure mitochondrial O2\textbullet - and H2O2 formation in vivo, as uncertainty about these values hampers studies on the role of mitochondrial ROS in pathological oxidative damage and redox signalling.},
  pmcid = {PMC2605959},
  pmid = {19061483},
  file = {/Users/naamanomar/Zotero/storage/KELCDHZD/Murphy - 2009 - How mitochondria produce reactive oxygen species.pdf}
}

