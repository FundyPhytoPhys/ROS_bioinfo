@incollection{declerckChapterTwoDiversity2012,
  title = {Chapter {{Two}} - {{Diversity}} and {{Evolution}} of {{Algae}}: {{Primary Endosymbiosis}}},
  shorttitle = {Chapter {{Two}} - {{Diversity}} and {{Evolution}} of {{Algae}}},
  booktitle = {Advances in {{Botanical Research}}},
  author = {De Clerck, Olivier and Bogaert, Kenny A. and Leliaert, Frederik},
