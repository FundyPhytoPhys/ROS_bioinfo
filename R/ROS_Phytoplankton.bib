@article{leblancNanoplanktonicDiatomsAre2018,
  title = {Nanoplanktonic Diatoms Are Globally Overlooked but Play a Role in Spring Blooms and Carbon Export},
  author = {Leblanc, Karine and Qu{\'e}guiner, Bernard and Diaz, Fr{\'e}d{\'e}ric and Cornet, V{\'e}ronique and {Michel-Rodriguez}, M{\'o}nica and {Durrieu de Madron}, Xavier and Bowler, Chris and Malviya, Shruti and Thyssen, Melilotus and Gr{\'e}gori, G{\'e}rald and Rembauville, Mathieu and Grosso, Olivier and Poulain, Julie and {de Vargas}, Colomban and {Pujo-Pay}, Mireille and Conan, Pascal},
