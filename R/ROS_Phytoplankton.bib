@article{ziyatdinovaReactionsSuperoxideAnion2005,
  title = {Reactions of Superoxide Anion Radical with Antioxidants and Their Use in Voltammetry},
  author = {Ziyatdinova, G. K. and Gil?metdinova, D. M. and Budnikov, G. K.},
  year = {2005},
