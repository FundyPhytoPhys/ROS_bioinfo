}

@article{hoppenrathThalassiosiraSpeciesBacillariophyceae2007,
  title = {{\emph{Thalassiosira}} {{Species}} ({{Bacillariophyceae}}, {{Thalassiosirales}}) in the {{North Sea}} at {{Helgoland}} ({{German Bight}}) and {{Sylt}} ({{North Frisian Wadden Sea}}) \textendash{} a {{First Approach}} to {{Assessing Diversity}}},
