@article{kobayashiVivoAntioxidantRole2000,
  title = {In Vivo Antioxidant Role of Astaxanthin under Oxidative Stress in the Green Alga {{{\emph{Haematococcus}}}}{\emph{ Pluvialis}}},
  author = {Kobayashi, M.},
  year = {2000},
