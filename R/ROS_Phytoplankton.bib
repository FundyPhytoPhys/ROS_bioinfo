  author = {{Aguilo-Ferretjans}, Maria del Mar and Bosch, Rafael and Puxty, Richard J. and Latva, Mira and Zadjelovic, Vinko and Chhun, Audam and Sousoni, Despoina and Polin, Marco and Scanlan, David J. and {Christie-Oleza}, Joseph A.},
