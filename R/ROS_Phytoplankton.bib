@article{giovannoniGenomeStreamliningCosmopolitan2005,
  title = {Genome {{Streamlining}} in a {{Cosmopolitan Oceanic Bacterium}}},
  author = {Giovannoni, Stephen J. and Tripp, H. James and Givan, Scott and Podar, Mircea and Vergin, Kevin L. and Baptista, Damon and Bibbs, Lisa and Eads, Jonathan and Richardson, Toby H. and Noordewier, Michiel and Rapp{\'e}, Michael S. and Short, Jay M. and Carrington, James C. and Mathur, Eric J.},
  year = {2005},
  month = aug,
  journal = {Science},
  volume = {309},
  number = {5738},
  pages = {1242--1245},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.1114057},
  abstract = {The SAR11 clade consists of very small, heterotrophic marine {$\alpha$}-proteobacteria that are found throughout the oceans, where they account for about 25\% of all microbial cells. Pelagibacter ubique, the first cultured member of this clade, has the smallest genome and encodes the smallest number of predicted open reading frames known for a free-living microorganism. In contrast to parasitic bacteria and archaea with small genomes, P. ubique has complete biosynthetic pathways for all 20 amino acids and all but a few cofactors. P. ubique has no pseudogenes, introns, transposons, extrachromosomal elements, or inteins; few paralogs; and the shortest intergenic spacers yet observed for any cell. A marine bacterium has a miniscule genome, free of junk DNA, probably because its huge population size allows selection against the small fitness cost of replicating nonfunctional DNA. A marine bacterium has a miniscule genome, free of junk DNA, probably because its huge population size allows selection against the small fitness cost of replicating nonfunctional DNA.},
  copyright = {American Association for the Advancement of Science},
  langid = {english},
  pmid = {16109880},
  file = {/Users/naamanomar/Zotero/storage/8VAKGUQK/Giovannoni et al. - 2005 - Genome Streamlining in a Cosmopolitan Oceanic Bact.pdf;/Users/naamanomar/Zotero/storage/FDW4UT2D/1242.html}
}

