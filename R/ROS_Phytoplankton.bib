@article{lifengOverexpressionChloroplasticMonodehydroascorbate2010,
  title = {Overexpression of Chloroplastic Monodehydroascorbate Reductase Enhanced Tolerance to Temperature and Methyl Viologen-mediated Oxidative Stresses},
