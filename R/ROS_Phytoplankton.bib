@article{hevia-orubeMolecularMorphologicalAnalyses2016,
  title = {Molecular and Morphological Analyses of Solitary Forms of Brackish {{Thalassiosiroid}} Diatoms ({{Coscinodiscophyceae}}), with Emphasis on Their Phenotypic Plasticity},
