@article{chepurnovSearchNewTractable2008,
  title = {In Search of New Tractable Diatoms for Experimental Biology},
  author = {Chepurnov, Victor A. and Mann, David G. and von Dassow, Peter and Vanormelingen, Pieter and Gillard, Jeroen and Inz{\'e}, Dirk and Sabbe, Koen and Vyverman, Wim},
  year = {2008},
  journal = {BioEssays},
  volume = {30},
