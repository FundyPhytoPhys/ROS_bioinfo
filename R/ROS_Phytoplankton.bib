@article{liangMolecularMechanismsTemperature2019,
  title = {Molecular Mechanisms of Temperature Acclimation and Adaptation in Marine Diatoms},
  author = {Liang, Yue and Koester, Julie A. and Liefer, Justin D. and Irwin, Andrew J. and Finkel, Zoe V.},
  year = {2019},
