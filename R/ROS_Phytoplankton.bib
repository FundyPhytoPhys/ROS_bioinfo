@article{amatoGrazerinducedTranscriptomicMetabolomic2018,
  title = {Grazer-Induced Transcriptomic and Metabolomic Response of the Chain-Forming Diatom {{{\emph{Skeletonema}}}}{\emph{ Marinoi}}},
