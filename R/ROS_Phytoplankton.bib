@article{wuDistinctivePhotosystemII2011,
  title = {Distinctive {{Photosystem II Photoinactivation}} and {{Protein Dynamics}} in {{Marine Diatoms}}},
  author = {Wu, Hongyan and Cockshutt, Amanda M. and McCarthy, Avery and Campbell, Douglas A.},
  year = {2011},
