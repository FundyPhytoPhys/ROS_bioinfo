  doi = {10.1016/B978-0-12-387817-5.00004-2},
  abstract = {Reactive oxygen and nitrogen species (ROS/RNS) are formed in the liver as a consequence of normal cellular processes (e.g., mitochondrial respiration and xenobiotic metabolism). Under physiological conditions, production of ROS/RNS is tightly regulated by endogenous antioxidants. However, when the balance between ROS/RNS production and antioxidant defense is disturbed, oxidative stress develops. In addition to causing direct cell injury, there is increasing evidence that oxidative stress can activate cell signaling pathways, leading to nuclear receptor activation, autophagy, mitochondrial dysfunction, or even cell death through apoptosis or necrosis. It is believed that these occur in the mechanisms of liver injury in many etiologies, and biomarkers of oxidative stress have become useful tools for mechanistic investigations in both basic and translational research. Here, we discuss the sources of ROS and RNS in the liver, the pathophysiological consequences of oxidative stress, endogenous antioxidant defenses, and the therapeutic potential of pharmacological antioxidants.},
  isbn = {978-0-12-387817-5},
  langid = {english},
  keywords = {antioxidants,apoptosis,autophagy,liver injury,mitochondria,necrosis,oxidative stress,reactive nitrogen,reactive oxygen},
  file = {/Users/naamanomar/Zotero/storage/2X86XK82/B9780123878175000042.html}
}

@article{medlinEvolutionDiatomsMajor2016,
  title = {Evolution of the Diatoms: Major Steps in Their Evolution and a Review of the Supporting Molecular and Morphological Evidence},
  shorttitle = {Evolution of the Diatoms},
  author = {Medlin, Linda K.},
  year = {2016},
  month = jan,
  journal = {Phycologia},
  volume = {55},
  number = {1},
  pages = {79--103},
  issn = {0031-8884},
  doi = {10.2216/15-105.1},
  abstract = {:  Over the years, many reviews of different aspects of diatom biology, ecology and evolution have appeared. Since 1993 many molecular trees have been produced to infer diatom phylogeny. In 2004, Medlin \& Kaczmarska revised the systematics of the diatoms based on more than 20 years of consistent recovery of two major clades of diatoms that did not correspond to a traditional concept of centrics and pennates and established three classes of diatoms: Clade 1 = Coscinodiscophyceae (radial centrics) and Clade 2 = Mediophyceae (polar centrics + radial Thalassiosirales) and Bacillariophyceae (pennates). However, under certain analytical conditions, an alternative view of diatom evolution, a grades of clades, has been recovered that suggests a gradual evolution from centric to pennate symmetry. These two schemes of diatom evolution are evaluated in terms of whether or not the criteria advocated by Medlin \& Kaczmarska that should be met to recover monophyletic classes have been used. The monophyly of the three diatom classes can only be achieved if (1) a secondary structure of the small subunit (SSU) rRNA gene was used to construct the alignment and not an alignment based on primary structure and (2) multiple outgroups were used. These requirements have not been met in each study of diatom evolution; hence, the grade of clades, which is useful in reconstructing the sequence of evolution, is not useful for accepting the new classification of the diatoms. Evidence for how these two factors affect the recovery of the three monophyletic classes is reviewed here. The three classes have been defined by clear morphological differences primarily based on gametangia and auxospore ontogeny and envelope structure, the presence or absence of a structure (tube process or sternum) associated with the annulus and the location of the cribrum in those genera with loculate areolae. New evidence supporting the three clades is reviewed. Other features of the cell are examined to determine whether they can also be used to support the monophyly of the three classes.},
  file = {/Users/naamanomar/Zotero/storage/76WNRT3Z/15-105.html}
}

@article{medlinEvolutionDiatomsMorphological2004,
  title = {Evolution of the Diatoms: {{V}}. {{Morphological}} and Cytological Support for the Major Clades and a Taxonomic Revision},
  shorttitle = {Evolution of the Diatoms},
  author = {Medlin, Linda K. and Kaczmarska, Irena},
  year = {2004},
  month = may,
  journal = {Phycologia},
  volume = {43},
  number = {3},
  pages = {245--270},
  issn = {0031-8884},
  doi = {10.2216/i0031-8884-43-3-245.1},
  abstract = {Phylogenetic reconstructions of the diatoms have been inferred with the 18S and 16S ribosomal RNA genes. Previous studies have shown that the group is divided into two major clades, with support coming initially from the arrangement of the Golgi bodies inside the cells in extant taxa. Features of extinct taxa that also support these clades can be found in the earliest fossil record of the diatoms and include the presence or absence of a central structure in the valve wall and the type of peripheral linking mechanisms between cells. Here we demonstrate that the general pattern of the auxospore expansion and the structure of their walls, the structure of the pyrenoid and the ultrastructure of the spermatozoid further support the molecular clades. Given the combined molecular and morphological support, we propose two new subdivisions (Coscinodiscophytina and Bacillariophytina), emend the classes Coscinodiscophyceae and Bacillariophyceae and propose a new class, the Mediophyceae for the bipolar centrics and the Thalassiosirales.},
  file = {/Users/naamanomar/Zotero/storage/IVLJKY75/i0031-8884-43-3-245.html}
}

