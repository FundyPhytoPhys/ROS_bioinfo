  langid = {english},
  file = {/Users/naamanomar/Zotero/storage/C7BVJBBG/362059a0.html}
}

@article{rosenwasserMappingDiatomRedoxSensitive2014,
  ids = {rosenwasserMappingDiatomRedoxsensitive2014},
  title = {Mapping the {{Diatom Redox-Sensitive Proteome Provides Insight}} into {{Response}} to {{Nitrogen Stress}} in the {{Marine Environment}}},
  author = {Rosenwasser, Shilo and {van Creveld}, Shiri Graff and Schatz, Daniella and Malitsky, Sergey and Tzfadia, Oren and Aharoni, Asaph and Levin, Yishai and Gabashvili, Alexandra and Feldmesser, Ester and Vardi, Assaf},
  year = {2014},
  month = feb,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {111},
  number = {7},
  pages = {2740--2745},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1319773111},
  abstract = {Diatoms are ubiquitous marine photosynthetic eukaryotes responsible for approximately 20\% of global photosynthesis. Little is known about the redox-based mechanisms that mediate diatom sensing and acclimation to environmental stress. Here we used a quantitative mass spectrometry-based approach to elucidate the redox-sensitive signaling network (redoxome) mediating the response of diatoms to oxidative stress. We quantified the degree of oxidation of 3,845 cysteines in the Phaeodactylum tricornutum proteome and identified approximately 300 redox-sensitive proteins. Intriguingly, we found redox-sensitive thiols in numerous enzymes composing the nitrogen assimilation pathway and the recently discovered diatom urea cycle. In agreement with this finding, the flux from nitrate into glutamine and glutamate, measured by the incorporation of 15N, was strongly inhibited under oxidative stress conditions. Furthermore, by targeting the redox-sensitive GFP sensor to various subcellular localizations, we mapped organelle-specific oxidation patterns in response to variations in nitrogen quota and quality. We propose that redox regulation of nitrogen metabolism allows rapid metabolic plasticity to ensure cellular homeostasis, and thus is essential for the ecological success of diatoms in the marine ecosystem.},
  copyright = {\textcopyright{} . Freely available online through the PNAS open access option.},
  langid = {english},
  pmid = {24550302},
  keywords = {marine diatoms,phytoplankton,redox proteomics,roGFP},
  file = {/Users/naamanomar/Zotero/storage/ACAK5N35/Rosenwasser et al. - 2014 - Mapping the diatom redox-sensitive proteome provid.pdf;/Users/naamanomar/Zotero/storage/PYKGQ9BL/2740.html}
}

