@article{foresiCharacterizationNitricOxide2010,
  title = {Characterization of a {{Nitric Oxide Synthase}} from the {{Plant Kingdom}}: {{NO Generation}} from the {{Green Alga Ostreococcus}} Tauri {{Is Light Irradiance}} and {{Growth Phase Dependent}}},
