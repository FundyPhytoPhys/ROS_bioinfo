@article{klosterMorphometryDiatomFragilariopsis2018,
  title = {Morphometry of the Diatom {{{\emph{Fragilariopsis}}}}{\emph{ Kerguelensis}} from {{Southern Ocean}} Sediment: {{High-throughput}} Measurements Show Second Morphotype Occurring during Glacials},
  shorttitle = {Morphometry of the Diatom {{Fragilariopsis}} Kerguelensis from {{Southern Ocean}} Sediment},
  author = {Kloster, Michael and Kauer, Gerhard and Esper, Oliver and Fuchs, Nike and Beszteri, B{\'a}nk},
  year = {2018},
  month = sep,
  journal = {Marine Micropaleontology},
  volume = {143},
  pages = {70--79},
  issn = {0377-8398},
  doi = {10.1016/j.marmicro.2018.07.002},
  abstract = {Fragilariopsis kerguelensis is one of the most abundant diatoms in the sediments of the Southern Ocean. Its morphometric features have been proposed as proxies, based on links with, for example, iron availability, sea surface temperature and glacial/interglacial conditions. We investigated morphometric changes in F. kerguelensis valves in a well-studied sediment core record, focusing on transition periods between glacials and interglacials. By applying a high-throughput diatom imaging and morphometry workflow, we found two clearly distinct morphotypes which were differentiated by their rectangularity. One of them occurred preferentially in glacial samples, whilst the other persisted throughout. This indicates that their relative abundances depend on environmental conditions and thereby points to the possibility that paleo-proxies based on F. kerguelensis valve morphometric features might in the future benefit from differentiating these two morphotypes. As an initial exploration of this idea, we show that the abundance ratio of both morphotypes correlates well with paleo-temperatures which had been reconstructed using independent data from the same core. Distinguishing between the two morphotypes only became possible by image analysis for precisely measuring diatom valve outlines and area, highlighting the potential of such methods for diatom analyses.},
  langid = {english},
  keywords = {Holocene,Paleoceanography,Proxy,Rectangularity,SHERPA,Valve area},
  file = {/Users/naamanomar/Zotero/storage/ZH4WFZRZ/Kloster et al. - 2018 - Morphometry of the diatom Fragilariopsis kerguelen.pdf;/Users/naamanomar/Zotero/storage/63YGFBXT/S0377839818300070.html}
}

