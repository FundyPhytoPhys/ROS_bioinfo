@article{astierNitricOxideProduction2021,
  title = {Nitric Oxide Production and Signalling in Algae},
  author = {Astier, Jeremy and Rossi, Jordan and Chatelain, Pauline and Klinguer, Agn{\`e}s and {Besson-Bard}, Ang{\'e}lique and Rosnoblet, Claire and Jeandroz, Sylvain and {Nicolas-Franc{\`e}s}, Val{\'e}rie and Wendehenne, David},
