@article{bagbyResponseProchlorococcusVarying2015,
  title = {Response of {{{\emph{Prochlorococcus}}}} to Varying {{CO}} 2 :{{O}} 2 Ratios},
  shorttitle = {Response of {{Prochlorococcus}} to Varying {{CO}} 2},
  author = {Bagby, Sarah C. and Chisholm, Sallie W.},
  year = {2015},
  month = oct,
  journal = {The ISME Journal},
  volume = {9},
  number = {10},
  pages = {2232--2245},
  publisher = {{Nature Publishing Group}},
  issn = {1751-7370},
  doi = {10.1038/ismej.2015.36},
  abstract = {Carbon fixation has a central role in determining cellular redox poise, increasingly understood to be a key parameter in cyanobacterial physiology. In the cyanobacterium Prochlorococcus\textemdash the most abundant phototroph in the oligotrophic oceans\textemdash the carbon-concentrating mechanism is reduced to the bare essentials. Given the ability of Prochlorococcus populations to grow under a wide range of oxygen concentrations in the ocean, we wondered how carbon and oxygen physiology intersect in this minimal phototroph. Thus, we examined how CO2:O2 gas balance influenced growth and chlorophyll fluorescence in Prochlorococcus strain MED4. Under O2 limitation, per-cell chlorophyll fluorescence fell at all CO2 levels, but still permitted substantial growth at moderate and high CO2. Under CO2 limitation, we observed little growth at any O2 level, although per-cell chlorophyll fluorescence fell less sharply when O2 was available. We explored this pattern further by monitoring genome-wide transcription in cells shocked with acute limitation of CO2, O2 or both. O2 limitation produced much smaller transcriptional changes than the broad suppression seen under CO2 limitation and CO2/O2 co-limitation. Strikingly, both CO2 limitation conditions initially evoked a transcriptional response that resembled the pattern previously seen in high-light stress, but at later timepoints we observed O2-dependent recovery of photosynthesis-related transcripts. These results suggest that oxygen has a protective role in Prochlorococcus when carbon fixation is not a sufficient sink for light energy.},
  copyright = {2015 International Society for Microbial Ecology},
  langid = {english},
  file = {/Users/naamanomar/Zotero/storage/NUNJGMJA/Bagby and Chisholm - 2015 - Response of Prochlorococcus to varying CO 2 O 2 r.pdf;/Users/naamanomar/Zotero/storage/GRA3L3P4/ismej201536.html}
}

@article{baierPlant2CysPeroxiredoxin1997,
  title = {The Plant 2-{{Cys}} Peroxiredoxin {{BAS1}} Is a Nuclear-Encoded Chloroplast Protein: Its Expressional Regulation, Phylogenetic Origin, and Implications for Its Specific Physiological Function in Plants},
  shorttitle = {The Plant 2-{{Cys}} Peroxiredoxin {{BAS1}} Is a Nuclear-Encoded Chloroplast Protein},
